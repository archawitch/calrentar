  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@configs/*": ["configs/*"],
      "@navigation/*": ["navigation/*"],
      "@screens/*": ["screens/*"],
